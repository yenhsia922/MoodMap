    <div class = "rows">
      <div class="col-xs-12">
        <div class="modal-dialog">
          <div class = "modal-content">
            <div class = "modal-header">
                <h4 class="modal-title" id="header-title">Edit Profile</h4>
            </div>

            <div class="modal-body">

              <div class="form-group">
                <div class="rows">
                  <div class="col-lg-12">
                    <input class="form-control input-lg" name="email" placeholder="ck11@ucsd.edu" type="text">  
                  </div>
                </div>
              </div>

              <div class="form-group">
                <div class="rows">
                  <div class="col-lg-12">

                    <input class="form-control input-lg" id="username" name="username" placeholder="JoanneW" type="text">  
                  </div>
                </div>
              </div>

              <div class="form-group">
                <div class="rows">
                  <div class="col-lg-12">
                    <input class="form-control input-lg" id="password" name="password" placeholder="***********" type="password">  
                  </div>
                </div>
              </div>

              <div class="form-group">
                <div class="rows">
                  <div class="col-lg-12">
                    <input class="form-control input-lg" id="interest" name="interest" placeholder=" #CS, #Music, #Swimming " type="text">  
                  </div>
                </div>
              </div>

              <div class="form-group">
                <div class="rows">
                  <div class="col-lg-12">
                    <div class="form-control">
                      <a class="dropdown"id="gender" role="button" data-toggle="dropdown" ><span class="selection">Female</span>
                      <span class="caret"></span></a>
                      <ul class="dropdown-menu" role="menu" aria-labelledby="dLabel">
                        <li><a tabindex="-1" href="#">Male</a></li>
                        <li><a tabindex="-1" href="#">Female</a></li>
                        <li><a tabindex="-1" href="#">Other</a></li>
                      </ul>
                    </div>
                 </div>
                </div>
              </div>

              <div class="form-group">
                <div class="rows">
                  <div class="col-lg-12">
                    <div class="form-control">
                      <div class="fileUpload">
                        <a class="upload" href='javascript:;'>Change Profile Picture
                        <input type="file"class="upload" onchange='$("#upload-file-info").html($(this).val());'/>
                        </a>
                      <span class='label label-info' id="upload-file-info">me.gif</span>
                      </div>
                    </div>
                  </div>
                </div>
              </div>


            <br>
        
          <!-- Button -->
          <div class="modal-footer">
            <button type="button" class="btn btn-default" data-dismiss="modal" id="submit" href="/MainPage">Done</button>
            <button type="button" class="btn btn-default" data-dismiss="modal" id="cancel" href="/MainPage">Cancel</button>
          </div>
        </div>
      </div>
    </div>
