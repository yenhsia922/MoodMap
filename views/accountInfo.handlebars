    <div class="container" id = "cont">
      <div id="header-title2">Edit Profile</div>
      <form class="form-horizontal"role="form" id="signupform"=>
        <!-- Text input-->
        <div class="form-group">
          <label class="control-label" for="email">Email:</label>  
          <input id="email" name="email" type="text" placeholder="introHCI170@ucsd.edu" class="form-control input-md">
        </div>
        <!-- Text input-->
        <div class="form-group">
          <label class="control-label" for="username">Username:</label>  
          <input id="username" name="username" type="text" placeholder="introHCI170" class="form-control input-md">
        </div>
        <!-- Password input-->
        <div class="form-group">
          <div class="left">
            <label class="control-label" for="password">Password: </label>
            <input id="password" name="password" type="password" placeholder="*******" class="form-control input-md">
          </div>
          <div class="right">
            <label class="control-label" for="password2">Confirm Password:</label>
            <input id="password2" name="password2" type="password" placeholder="*******" class="form-control input-md" >
          </div>
        </div>
        <!-- Text input-->
        <div class="form-group">
          <div class="left">
            <label class="control-label" for="age">Age:</label>  
            <input id="age" name="age" type="text" placeholder="20" class="form-control input-md">
          </div>
          <div class="right">
            <label class="control-label" for="Birthday">Birthday:</label>  
            <input id="birthday" name="birthday" type="text" placeholder="02/27/1995" class="form-control input-md">
          </div>
        </div>

        <!-- Multiple Radios -->
        <div class="form-group">
          <label class="control-label" for="gender">Gender:</label>
          <div class="radio">
            <label for="gender-0">
            <input type="radio" name="gender" id="gender-0" value="1" >
            Male
            </label>
            <label for="gender-1">
            <input type="radio" name="gender" id="gender-1" value="2" checked="checked">
            Female
            </label>
            <label for="gender-2">
            <input type="radio" name="gender" id="gender-2" value="3">
            Other
            </label>
          </div>
        </div>
        <!-- Textarea -->
        <div class="form-group">
          <label class="control-label" for="interests">Interests:</label>
          <textarea class="form-control" id="interests" name="interests" placeholder="#CS,#VolleyBall,#BBQ" ></textarea>
        </div>
        <!-- File Button --> 
        <div class="form-group">
          <label class="control-label" for="filebutton">Upload Profile Picture</label>
          <input id="filebutton" name="filebutton" class="input-file" type="file">
        </div>
        <br>
        <!-- Button -->
        <div class="form-group">
          <div class="submit-button">
          <a class="btn btn-default" id="done"href="JavaScript:window.close()" role="button">Save</a>
          <a class="btn btn-default" href="JavaScript:window.close()" role="button">Cancel</a>
        </div>
        </div>
      </form>
